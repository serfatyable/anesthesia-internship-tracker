{
  "name": "anesthesia-internship-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "next dev",
    "dev:setup": "pnpm db:health && pnpm dev",
    "dev:auto": "pnpm exec tsx scripts/start-dev.ts",
    "startup:check": "tsx scripts/startup-check.ts",
    "build": "next build",
    "postbuild": "prisma generate",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --force",
    "db:reset:full": "pnpm db:reset && pnpm db:seed",
    "db:health": "tsx scripts/check-db-health.ts",
    "db:studio": "prisma studio",
    "db:setup": "pnpm db:push && pnpm db:seed",
    "prisma:generate": "prisma generate",
    "prisma:validate": "prisma validate",
    "prisma:format": "prisma format",
    "postinstall": "prisma generate",
    "peek": "tsx scripts/peek.ts",
    "integration-test": "tsx scripts/integration-test.ts",
    "prepare": "husky",
    "doctor": "tsx scripts/doctor.ts",
    "setup:dev": "tsx scripts/setup-dev.ts",
    "build:production": "tsx scripts/build-production.ts",
    "optimize:production": "tsx scripts/optimize-production.ts",
    "validate:env": "tsx scripts/validate-env.ts",
    "docker:build": "docker build -t anesthesia-tracker .",
    "docker:run": "docker run -p 3000:3000 anesthesia-tracker",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "deploy:vercel": "vercel --prod",
    "deploy:staging": "vercel --target staging",
    "health:check": "curl -f http://localhost:3000/api/health",
    "monitor:metrics": "curl -f http://localhost:3000/api/monitoring/metrics",
    "monitor:errors": "curl -f http://localhost:3000/api/monitoring/errors",
    "security:audit": "pnpm audit --audit-level moderate",
    "performance:test": "tsx scripts/performance-test.ts",
    "backup:db": "pg_dump $DATABASE_URL > backup_$(date +%Y%m%d_%H%M%S).sql",
    "restore:db": "psql $DATABASE_URL < backup_file.sql"
  },
  "dependencies": {
    "@auth/core": "0.34.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "@types/redis": "^4.0.11",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.63.0",
    "redis": "^5.8.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.16.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.11",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prisma": "^6.16.2",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed-minimal.ts"
  }
}
